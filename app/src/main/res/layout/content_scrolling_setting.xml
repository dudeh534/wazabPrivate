<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_mypage">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#f2f3f3"
        android:layout_marginTop="33dp">

        <ImageView
            android:id="@+id/sPro"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/icon_user"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/linearLayout10"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:orientation="vertical"
            android:layout_gravity="center">

            <com.ourincheon.wazap.NotoTextView
                android:id="@+id/eName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:hint="이름"
                android:textSize="17dp"
                android:textColor="#363636" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="17dp"
                android:layout_marginTop="4dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView21"
                    android:layout_gravity="center"
                    android:src="@drawable/detail_icon_kakao"
                    android:layout_marginRight="3dp" />

                <EditText
                    android:id="@+id/eKakao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/sLoc"
                    android:layout_alignStart="@+id/sLoc"
                    android:layout_below="@+id/sLoc"
                    android:layout_gravity="center"
                    android:hint="카카오 아이디"
                    android:textSize="10dp"
                    android:textColor="#363636" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/linearLayout10"
            android:orientation="vertical"
            android:background="#ffffff"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="5dp">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView13"
                    android:src="@drawable/detail_icon_college"
                    android:layout_gravity="center"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="18dp" />

                <EditText
                    android:hint="학교"
                    android:id="@+id/eUniv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/sIntro"
                    android:layout_toEndOf="@+id/linearLayout10"
                    android:layout_toRightOf="@+id/linearLayout10"
                    android:textSize="13dp"
                    android:layout_gravity="center"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#f2f3f3"></LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView18"
                    android:src="@drawable/detail_icon_book"
                    android:layout_gravity="center"
                    android:layout_marginLeft="23dp"
                    android:layout_marginRight="20dp" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/linearLayout10"
                    android:layout_alignParentBottom="true"
                    android:layout_alignStart="@+id/linearLayout10"
                    android:hint="전공 및 희망역할"
                    android:id="@+id/eMajor"
                    android:textSize="13dp"
                    android:layout_gravity="center"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#f2f3f3" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView19"
                    android:src="@drawable/detail_icon_position"
                    android:layout_gravity="center"
                    android:layout_marginLeft="22dp"
                    android:layout_marginRight="19dp" />

                <EditText
                    android:id="@+id/eSkill"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/sUniv"
                    android:layout_alignLeft="@+id/sUniv"
                    android:layout_alignStart="@+id/sUniv"
                    android:hint="포지션"
                    android:textSize="13dp"
                    android:layout_gravity="center"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#f2f3f3" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView20"
                    android:src="@drawable/detail_icon_location"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24dp"
                    android:layout_marginRight="21dp" />

                <EditText
                    android:id="@+id/eLoc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/sExp"
                    android:layout_alignBottom="@+id/sExp"
                    android:layout_centerHorizontal="true"
                    android:hint="지역"
                    android:textSize="13dp"
                    android:layout_gravity="center"
                    android:textColor="#000000" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="5dp">

            <com.ourincheon.wazap.NotoTextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:text="자기소개"
                android:textSize="13dp"
                android:layout_gravity="center_horizontal"
                android:textColor="#0057ff"
                android:layout_marginTop="23dp"
                android:layout_marginBottom="23dp" />

            <EditText
                android:id="@+id/eIntro"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="top"
                android:lines="3"
                android:singleLine="false"
                android:textSize="12dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="1dp">

            <com.ourincheon.wazap.NotoTextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:text="기타"
                android:textSize="13dp"
                android:layout_gravity="center_horizontal"
                android:textColor="#0057ff"
                android:layout_marginTop="23dp"
                android:layout_marginBottom="23dp"/>

            <EditText
                android:id="@+id/eExp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="top"
                android:lines="3"
                android:singleLine="false"
                android:textSize="12dp" />
        </LinearLayout>

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>
